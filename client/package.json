{
    "name": "millionaire",
    "version": "1.0.7",
    "license": "ISC",
    "author": "Danyil Orel",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/danorel/Millionaire.git"
    },
    "bugs": {
        "url": "https://github.com/danorel/Millionaire/issues"
    },
    "description": "Golang web application as technical assignment",
    "keywords": [
        "Golang",
        "React"
    ],
    "main": "index.tsx",
    "scripts": {
        "test": "jest",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "eslint": "tsc --noEmit && eslint src/**/*.ts{,x} --fix",
        "formatting": "prettier --write \"src/**/*.{js,jsx,ts,tsx,html,css,json}\" --ignore-unknown"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*": [
            "npm run formatting"
        ],
        "src/**/*.{js,jsx,ts,tsx,html,css,json}": [
            "npm run eslint",
            "git add"
        ]
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.5.1",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.2.1",
        "@types/jest": "^24.9.1",
        "@types/node": "^12.20.7",
        "@types/react": "^16.14.5",
        "@types/react-dom": "^16.9.12",
        "@types/react-redux": "^7.1.16",
        "@types/react-router-dom": "^5.1.7",
        "axios": "^0.21.1",
        "jest": "^26.6.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.3",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "ts-jest": "^26.5.4",
        "typescript": "^4.1.5"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.21.0",
        "@typescript-eslint/parser": "^4.21.0",
        "eslint": "^7.24.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.3.4",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^1.7.0",
        "http-proxy-middleware": "^1.1.1",
        "husky": "^4.3.8",
        "lint-staged": "^10.5.4",
        "prettier": "2.2.1"
    }
}
